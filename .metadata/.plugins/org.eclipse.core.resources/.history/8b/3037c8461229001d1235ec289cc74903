package trans.util;

public class Protocol
{
	public enum Type 
	{
		DIR(0), FILE(1);

		private final int i;
		Type(int i)
		{
			this.i = i;
		}
		
		public int getValue() { return i; }
	}
	
	public static void main(String[] args)
	{
		String path = "â€ªC:\\TMTest\\source\\abc.gif";
		byte[] b = path.getBytes();
		
		System.out.println(b.length);
	}
}
